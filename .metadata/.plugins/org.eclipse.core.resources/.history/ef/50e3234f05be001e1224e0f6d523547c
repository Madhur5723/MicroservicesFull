spring:
  application:
    name: API-Gateway






  cloud:
    gateway:
      routes:
        - id: employee-Service
          uri: lb://employee-Service
          predicates:
            - Path=/one/**
        - id: ADDRESS-SERVICE
          uri: lb://ADDRESS-SERVICE
          predicates:
            - Path=/two/**

server:
  port: 9093
eureka:
  instance:
    hostname: localhost